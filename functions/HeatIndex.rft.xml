<?xml version="1.0" encoding="utf-8"?>
<RasterFunctionTemplate xsi:type="typens:RasterFunctionTemplate" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:typens="http://www.esri.com/schemas/ArcGIS/10.3">
  <Name>HeatIndex</Name>
  <Description>A raster function template that combines ambient air temperature and relative humidity to return apparent temperature.</Description>
  <Function xsi:type="typens:PythonAdapterFunction">
    <Name>HeatIndex</Name>
    <Description></Description>
    <PixelType>F32</PixelType>
  </Function>
  <Arguments xsi:type="typens:PythonAdapterFunctionArguments">
    <Names xsi:type="typens:ArrayOfString">
      <String>PythonModule</String>
      <String>ClassName</String>
      <String>temperature</String>
      <String>rh</String>
      <String>units</String>
      <String>outunits</String>
    </Names>
    <Values xsi:type="typens:ArrayOfAnyType">
      <AnyType xsi:type="xs:string">HeatIndex.py</AnyType>
      <AnyType xsi:type='xs:string'>HeatIndex</AnyType>
      <AnyType xsi:type="typens:RasterFunctionVariable">
        <Name>Temperature</Name>
        <Description>A single-band raster where pixel values represent ambient air temperature.</Description>
        <Value></Value>
        <IsDataset>true</IsDataset>
        <Aliases xsi:type="typens:ArrayOfString">
          <String>Raster1</String>
          <String>t@SFC</String>
        </Aliases>
      </AnyType>
      <AnyType xsi:type="typens:RasterFunctionVariable">
        <Name>RH</Name>
        <Description>A single-band raster where pixel values represent relative humidity as a percentage value between 0 and 100.</Description>
        <Value></Value>
        <IsDataset>true</IsDataset>
        <Aliases xsi:type="typens:ArrayOfString">
          <String>Raster2</String>
          <String>rh@SFC</String>
        </Aliases>
      </AnyType>
      <AnyType xsi:type='typens:RasterFunctionVariable'>
        <Name>Temperature-Units</Name>
        <Description>The unit of measurement associated with the input temperature raster.</Description>
        <Value xsi:type='xs:string'>Fahrenheit</Value>
        <IsDataset>false</IsDataset>
      </AnyType>
      <AnyType xsi:type='typens:RasterFunctionVariable'>
        <Name>Output-Units</Name>
        <Description>The unit of measurement associated with the output heat-index raster.</Description>
        <Value xsi:type='xs:string'>Fahrenheit</Value>
        <IsDataset>false</IsDataset>
      </AnyType>      
    </Values>
  </Arguments>
  <Help></Help>
  <Type>2</Type>
  <Thumbnail></Thumbnail>
  <Definition></Definition>
  <Group>GroupName</Group>
  <Tag>Tag</Tag>
</RasterFunctionTemplate>